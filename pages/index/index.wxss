/**index.wxss**/
.container {
  background-color: #fff;
}
.common_title {
  font-size: 16px!important;
  flex: inherit!important;
}

.box {
  position: relative;
  height: 400rpx;
}

swiper {
  position: relative;
  height: 400rpx;
}

.wx-swiper-dots {
  position: relative;
  left: unset !important;
  right: 0;
  top: 272rpx;
}

.slide-image {
  width: 100%;
  height: 400rpx;
}

.search {
  position: absolute;
  top: 40rpx;
  right: 24rpx;
}

.search image {
  width: 36rpx;
  height: 36rpx;
}

.location {
  position: absolute;
  top: 40rpx;
  left: 24rpx;
}

.location_box {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 16rpx;
  height: 46rpx;
  border-radius: 8rpx;
  text-align: center;
  font-size: 14px;
  color: #fff;
  background-color: rgba(0, 0, 0, .2);
}

.location_box image {
  margin-left: 8rpx;
  width: 16rpx;
  height: 10rpx;
}

.wrap {
  padding: 0 24rpx 24rpx;
  width: 680rpx;
  margin: 0 auto;
  border-radius: 20rpx;
  box-shadow: 0 12rpx 24rpx 2rpx rgba(4, 0, 0, .06);
  background-color: #fff;
}

.publish {
  position: relative;
  top: -100rpx;
  margin-bottom: -100rpx;
}

.area_ht {
  height: 160rpx!important;
}

.publish_confirm {
  padding: 30rpx 0;
  margin-bottom: 20rpx;
}

.publish_btn {
  border-radius: 100rpx;
  color: #fff;
  background-color: transparent;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 100%;
}

.tips {
  overflow: hidden;
  margin-top: 24rpx;
  margin-bottom: 24rpx;
  text-align: center;
}

.tips_icon {
  display: block;
  width: 36rpx;
  height: 36rpx;
  margin: 24rpx auto;
}

.tips_txt {
  font-size: 12px;
  line-height: 40rpx;
  color: #8c8c8c;
}

.ensure_title {
  position: relative;
  text-align: center;
  font-size: 18px;
  padding: 24rpx 0;
}

.ensure_title:before,
.ensure_title:after {
  content: '';
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -1rpx;
  width: 44rpx;
  height: 2rpx;
  background-color: #e5e5e5;
}

.ensure_title:before {
  left: 50%;
  margin-left: 160rpx;
}

.ensure_title:after {
  right: 50%;
  margin-right: 160rpx;
}

.ensure_icon {
  display: block;
  width: 52rpx;
  height: 52rpx;
  margin: 0 auto 20rpx;
}

.ensure_list {
  padding: 24rpx;
}

.ensure_txt {
  font-size: 12px;
  text-align: center;
}

.hotel_list {
  padding: 16rpx 0;
  /* overflow-x: scroll;
  overflow-y: hidden; */
  white-space: nowrap;
}

.hotel_item:nth-child(1) {
  margin-left: 24rpx;
}

.hotel_item {
  display: inline-block;
  width: 308rpx;
  margin-right: 24rpx;
}

.hotel_pic {
  display: block;
  margin: 0 auto;
  width: 308rpx;
  height: 230rpx;
}

.hotel_name {
  font-size: 14px;
  margin-top: 16rpx;
}

.hotel_common {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  line-height: 40rpx;
  font-size: 12px;
  color: #999;
}